{
    "builds": [
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        },
        {
            "src": "/",
            "dest": "/frontend/index.html"
        }
    ],
    "regions": [
        "gru1"
    ]
}